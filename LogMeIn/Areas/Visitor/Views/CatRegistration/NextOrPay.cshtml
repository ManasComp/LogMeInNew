@model int

<div class="text-center justify-content-center align-items-center">
    <h1 class="row text-center justify-content-center text-primary p-5">Co dále?</h1>
    <div class="row text-center justify-content-center">
    </div>
    <div class="container px-4 text-center justify-content-center align-items-center">
        <div class="row justify-content-center align-items-center">

            <div class="col col-lg-4">
                <a class="col col-lg-4 btn btn-primary img-fluid w-100" asp-area="Visitor" asp-controller="CatRegistration" asp-action="PayInformation" asp-route-personRegistrationId="@Model">
                    <h1>Závazně dokončit a přejít k platbě</h1>
                </a>
            </div>

            <div class="d-none d-lg-block col-2 p-3w-25 justify-content-center align-items-center text-center" style="align-self: center">
                @* <img src=" https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcR0dbwoyaUABQ2k9M6rTWN_4MQI6mF45ODNWA&usqp=CAU" class="img-fluid align-items-center"/> *@
                <img src="https://img.icons8.com/pastel-glyph/100/cat--v3.png" alt="cat--v3" style="align-self: center"/>
                <div> <a href="https://icons8.com/icon/121371/cat">Cat</a> icon by <a href="https://icons8.com">Icons8</a></div>
            </div>

            <div class="col col-lg-4">
                <a class="col col-lg-4 btn btn-primary img-fluid w-100" asp-area="Visitor" asp-controller="CatRegistration" asp-action="CreateCatRegistration">
                    <h1>Zaregistrovar další kočku</h1>
                </a>
            </div>


        </div>
    </div>
</div>